<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../relaxng/xcl2.rnc"?>
<Construct xmlns="http://purl.org/xcl/2.0/">
<Forall>
  <Name>x</Name>
  <Name>y</Name>
  <Exists>
    <Name>P</Name>
    <Implies>
      <Not>
        <Atom>
          <Name>P</Name>
          <Name>x</Name>
          <Name>y</Name>
        </Atom>
      </Not>
      <And>
        <Equal>
          <Name>x</Name>
          <Name>y</Name>
        </Equal>
        <Or>
          <Atom><Name>P</Name></Atom>
          <Atom><Name>Q</Name></Atom>
        </Or>
      </And>
    </Implies>
  </Exists>
</Forall>
</Construct>
