<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../relaxng/xcl2.rnc"?>
<Construct xmlns="http://purl.org/xcl/2.0/">
  <Comment>Example 5 (CLIF).
    (cl:text
      (cl:import O1)
      (forall (x y) (iff (Reflexive x)(x y y))) 
      ( Reflexive P)
      ( isAxiomatizedIn P O1 )
     )
  </Comment>
  <Import>
    <Name>O1</Name>    
  </Import>
  <Forall>
    <Name>x</Name>
    <Name>y</Name>
    <Equivalent>
      <Atom>
        <Name>Reflexive</Name>
        <Name>x</Name>
      </Atom>
      <Atom>
        <Name>x</Name>
        <Name>y</Name>
        <Name>y</Name>
      </Atom>
    </Equivalent>
  </Forall>
  <Atom>
    <Name>Reflexive</Name>
    <Name>P</Name>
  </Atom>
  <Atom>
    <Name>isAxiomatizedIn</Name>
    <Name>P</Name>
    <Name>O1</Name>
  </Atom>
</Construct>
