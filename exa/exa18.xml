<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../relaxng/xcl2.rnc"?>
<TextConstruction xmlns="http://iso-commonlogic.org/xcl2">
  <Comment>Example 18 
    (txt (outDiscourse W P ) (imports O8)
    ∀x∀y((W x y) ↔ (P y x)))
    )
  </Comment>
  <OutDiscourse>
    <Name>W</Name>
    <Name>P</Name>
  </OutDiscourse>
  <Importation>
    <Name>O8</Name>    
  </Importation>
  <Universal>
    <Name>x</Name>
    <Name>y</Name>
    <Biconditional>
      <Atom>
        <Name>W</Name>
        <Name>x</Name>
        <Name>y</Name>
      </Atom>
      <Atom>
        <Name>P</Name>
        <Name>y</Name>
        <Name>x</Name>
      </Atom>
    </Biconditional>
  </Universal>
</TextConstruction>
