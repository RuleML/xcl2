<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../relaxng/xcl2.rnc"?>
<Construct xmlns="http://purl.org/xcl/2.0/">
  <Prefix pre="xsd" iri="http://www.w3.org/2001/XMLSchema"/>
  <Comment>Example 9.
    (cl:module PositiveIntegers (not (π &lt; 4)))
  </Comment>
  <Restrict>
    <Name>PositiveIntegers</Name>
    <Construct>
        <Not>
          <Atom>
            <Name>π</Name>
            <Name>&lt;</Name>
            <Data datatype="xsd:decimal">4</Data>
          </Atom>
        </Not>
    </Construct>
  </Restrict>
</Construct>
