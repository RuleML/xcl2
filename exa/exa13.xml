<?xml-model href="../relaxng/xcl2.rnc"?>
<Construct xmlns="http://purl.org/xcl/2.0/">
  <Comment>Example13. ( if (P a) ( exists (x) (P x)))</Comment>
  <Implies>
    <Atom>
      <Name>P</Name>
      <Name>a</Name>
    </Atom>
    <Exists>
      <Name>x</Name>
      <Atom>
        <Name>P</Name>
        <Name>x</Name>
      </Atom>
    </Exists>
  </Implies>
</Construct>
